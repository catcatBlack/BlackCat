<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.model.ServiceModel">
    <select id="userget" parameterType="Integer" resultType="com.Model.ServiceModel.userGetModel">
        select * from userget
        where user_id = #{userId};
    </select>

</mapper>